{
  "name": "uniprot-website",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://ebi-uniprot.github.io/uniprot-website/",
  "main": "./src/ui/index.tsx",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config webpack.dev.js",
    "build": "webpack --mode production --config webpack.prod.js",
    "check-types": "tsc",
    "lint": "eslint 'src/**/*.ts*' --ignore-pattern 'src/**/*.spec.ts*'",
    "lint-fix": "eslint 'src/**/*.ts*' --ignore-pattern 'src/**/*.spec.ts*' --fix",
    "analyse": "webpack-bundle-analyzer build/stats.json",
    "test": "yarn check-types && yarn lint && jest",
    "clear-test-cache": "jest --clearCache"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 11"
  ],
  "jest": {
    "transform": {
      "^.+\\.(tsx|js|ts)?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lit-html).+\\.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^react$": "<rootDir>/node_modules/react/",
      "^react-router-dom$": "<rootDir>/node_modules/react-router-dom/"
    }
  },
  "dependencies": {
    "@geneontology/ribbon": "^1.9.32",
    "@sentry/browser": "^5.6.3",
    "@swissprot/rhea-reaction-visualizer": "^0.0.2",
    "@types/lodash": "^4.14.149",
    "axios": "^0.18.0",
    "core-js": "^3.1.4",
    "d3": "^5.9.2",
    "franklin-sites": "0.0.29",
    "idx": "^2.5.2",
    "interaction-viewer": "^2.2.4",
    "lit-html": "^1.1.2",
    "litemol": "^2.4.2",
    "lodash": "^4.17.15",
    "protvista-datatable": "^2.2.15",
    "protvista-filter": "^2.1.17",
    "protvista-manager": "^2.1.13",
    "protvista-navigation": "^2.1.13",
    "protvista-sequence": "^2.1.17",
    "protvista-structure": "^2.1.18",
    "protvista-track": "^2.1.17",
    "protvista-variation": "^2.1.17",
    "protvista-variation-adapter": "^2.3.0",
    "query-string": "^6.2.0",
    "react": "^16.9.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-router-hash-link": "^1.2.2",
    "redux": "^4.0.1",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.20",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "typesafe-actions": "^3.0.0",
    "url-join": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@svgr/webpack": "^5.0.0",
    "@testing-library/jest-dom": "^4.2.1",
    "@testing-library/react": "^9.3.0",
    "@types/d3": "^5.7.2",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.9.2",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.3",
    "jest": "^23.6.0",
    "jest-css-modules-transform": "^2.1.1",
    "node-sass": "^4.9.3",
    "path": "^0.12.7",
    "react-beautiful-dnd-test-utils": "^2.0.0",
    "react-test-renderer": "^16.9.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^23.10.5",
    "typescript": "<3.6.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.1.4"
  }
}
