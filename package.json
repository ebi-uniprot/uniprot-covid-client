{
  "name": "uniprot-website",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://ebi-uniprot.github.io/uniprot-website/",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --env.PUBLIC_PATH=/ --mode development --open",
    "build:prod": "webpack --env.PUBLIC_PATH=/ --mode production",
    "build:staging": "webpack --env.PUBLIC_PATH=/uniprot-website/ --mode production",
    "build:dev": "webpack --env.PUBLIC_PATH=/ --mode development",
    "lint-fix": "eslint 'src/**/*.ts*' --ignore-pattern 'src/**/*.spec.ts*' --fix",
    "analyse": "webpack-bundle-analyzer build/stats.json",
    "test": "npm-run-all --continue-on-error test:*",
    "test:lint": "eslint 'src/**/*.ts*' --ignore-pattern 'src/**/*.spec.ts*' --cache --cache-location 'node_modules/.cache'",
    "test:types": "tsc",
    "test:unit": "jest",
    "test:functional": "jest --config tests/jest.config.json tests/functional",
    "test:visual": "jest --config tests/jest.config.json tests/visual",
    "clear-test-cache": "jest --clearCache",
    "update-snapshots": "jest -u"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 11"
  ],
  "jest": {
    "transform": {
      "^.+\\.(tsx|js|ts)?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lit-html).+\\.js"
    ],
    "testPathIgnorePatterns": [
      "/__mocks__/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^react$": "<rootDir>/node_modules/react/",
      "^react-router-dom$": "<rootDir>/node_modules/react-router-dom/",
      "^lodash-es$": "lodash"
    },
    "globals": {
      "BASE_URL": "/"
    },
    "snapshotSerializers": [
      "<rootDir>/jest.serializer.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm-run-all test:lint test:types \"test:unit --silent --reporters='jest-silent-reporter' --bail\""
    }
  },
  "lint-staged": {
    "*.+(ts|tsx|js|jsx|css|scss)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@geneontology/ribbon": "1.11.2",
    "@sentry/browser": "5.15.5",
    "@swissprot/rhea-reaction-visualizer": "0.0.12",
    "axios": "0.19.2",
    "d3": "5.16.0",
    "d3-scale": "3.2.1",
    "franklin-sites": "0.0.56",
    "history": "4.10.1",
    "idb": "5.0.4",
    "idx": "2.5.6",
    "interaction-viewer": "2.11.0",
    "lit-html": "1.2.1",
    "litemol": "2.4.2",
    "lodash-es": "4.17.15",
    "protvista-datatable": "2.11.0",
    "protvista-filter": "2.11.0",
    "protvista-manager": "2.11.0",
    "protvista-navigation": "2.11.0",
    "protvista-sequence": "2.11.0",
    "protvista-structure": "2.11.0",
    "protvista-track": "2.11.0",
    "protvista-uniprot": "2.5.4",
    "protvista-utils": "2.11.0",
    "protvista-variation": "2.11.0",
    "protvista-variation-adapter": "2.11.0",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-beautiful-dnd": "13.0.0",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "react-router-hash-link": "1.2.2",
    "react-spring": "8.0.27",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-persist-transform-filter": "0.0.20",
    "redux-thunk": "2.3.0",
    "timing-functions": "2.0.0",
    "typesafe-actions": "5.1.0",
    "url-join": "4.0.1",
    "uuid": "8.0.0",
    "workbox-cacheable-response": "5.1.3",
    "workbox-core": "5.1.3",
