{
  "name": "uniprot-website",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://ebi-uniprot.github.io/uniprot-website/",
  "main": "./src/ui/index.tsx",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config webpack.dev.js",
    "build": "webpack --mode production --config webpack.build.js",
    "build:dev": "webpack --mode development --config webpack.build.js",
    "check-types": "tsc",
    "lint": "eslint 'src/**/*.ts*' --ignore-pattern 'src/**/*.spec.ts*'",
    "lint-fix": "eslint 'src/**/*.ts*' --ignore-pattern 'src/**/*.spec.ts*' --fix",
    "analyse": "webpack-bundle-analyzer build/stats.json",
    "test": "yarn check-types && yarn lint && jest",
    "clear-test-cache": "jest --clearCache"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 11"
  ],
  "jest": {
    "transform": {
      "^.+\\.(tsx|js|ts)?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lit-html).+\\.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^react$": "<rootDir>/node_modules/react/",
      "^react-router-dom$": "<rootDir>/node_modules/react-router-dom/"
    },
    "globals": {
      "BASE_URL": "/"
    },
    "snapshotSerializers": [
      "<rootDir>/jest.serializer.js"
    ]
  },
  "dependencies": {
    "@geneontology/ribbon": "1.11.2",
    "@sentry/browser": "5.15.5",
    "@swissprot/rhea-reaction-visualizer": "0.0.2",
    "@types/lodash": "4.14.150",
    "axios": "0.19.2",
    "d3": "5.16.0",
    "d3-scale": "3.2.1",
    "franklin-sites": "0.0.44",
    "history": "4.10.1",
    "idx": "2.5.6",
    "interaction-viewer": "2.7.0",
    "lit-html": "1.2.1",
    "litemol": "2.4.2",
    "lodash": "4.17.15",
    "protvista-datatable": "2.8.2",
    "protvista-filter": "2.7.0",
    "protvista-manager": "2.9.1",
    "protvista-navigation": "2.3.2",
    "protvista-sequence": "2.7.0",
    "protvista-structure": "2.9.1",
    "protvista-track": "2.6.1",
    "protvista-uniprot": "2.5.4",
    "protvista-variation": "2.6.1",
    "protvista-variation-adapter": "2.3.3",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-beautiful-dnd": "13.0.0",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-router-hash-link": "1.2.2",
    "react-spring": "8.0.27",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-persist-transform-filter": "0.0.20",
    "redux-thunk": "2.3.0",
    "typesafe-actions": "5.1.0",
    "url-join": "4.0.1",
    "uuid": "7.0.3",
    "workbox-cacheable-response": "5.1.3",
    "workbox-core": "5.1.3",
    "workbox-expiration": "5.1.3",
    "workbox-precaching": "5.1.3",
    "workbox-routing": "5.1.3",
    "workbox-strategies": "5.1.3"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.5",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@svgr/webpack": "5.4.0",
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/react": "10.0.3",
    "@testing-library/react-hooks": "3.2.1",
    "@types/d3": "5.7.2",
